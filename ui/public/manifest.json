{
  "name": "Two Captions for YouTube & Netflix",
  "description": "Show captions in two languages on Youtube and Netflix",
  "version": "1.2.1.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*", "storage"
  ],
  "browser_action": {
      "default_title": "Show dual captions",
      "default_icon": "icon.png",
      "default_popup": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2
}
